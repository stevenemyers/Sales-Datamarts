                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT cust.CustID
	,CustName
	,ISNULL(cust.SperID,'') as SperID
	,ISNULL(sp.[SperName],'') as SalesPerson
	,ISNULL(cust.[SperKey],'') as [SperKey]
	,CASE SP.SperID 	
	When 'HA' THEN 'House'
	When 'SP1' THEN 'HH'
	When 'SP2' THEN 'Transit'
	When 'SP3' THEN 'HH'
	When 'SP4' THEN 'HH'
	When 'SP5' THEN 'Transit'
	When 'SP6' THEN 'HH'
	When 'SP7' THEN 'Transit'
	When 'SP9' THEN 'House'
	When 'SP8' THEN 'House'
	When 'SP10' THEN 'HH'
	ELSE ''
	END	
	As CustType

  FROM [dbo].[vdvCustomer] cust 
   left outer join  [dbo].[vdvSalesperson] as sp on sp.[SperKey] = cust.[SperKey]
   

  Where cust.CompanyID = 'USA' --and CreateDate &gt;= '1/1/2016'
  AND cust.CustID &lt;&gt; 'NEW CUSTOMER'
 -- and SperID is null

Order By cust.CustID</property>
