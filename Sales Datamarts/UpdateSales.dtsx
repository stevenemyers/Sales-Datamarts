DECLARE @invec AS Table   
(
TranNo varchar(20) Primary Key,
CustID varchar(20),
CustName varchar(50),
ENdCust varchar(20),
EndCustName varchar(50),
CustType varchar(20),
[LineCmnt] [varchar](255) NULL,
[HdrCmnt] [varchar](255) NULL
)
 
INsert into @invec 
Select Distinct INV.TranNo
,ISNULL(CUST.CustID, '') as CustID, ISNULL(CUST.CustName,'') as CustName,
rtrim(case when ISNULL(NA.NationalAcctId,'') = '' then CASE WHEN  ISNULL(SO.UserFld1,'') = '' THEN CUST.CustID ELSE  SO.UserFld1 END
else NA.NationalAcctId end) as [EndCust],
rtrim(case when ISNULL(NA.NationalAcctId ,'') = '' then case when ISNULL(ENDCUST.CustName,'') = ''  then ISNULL(SO.UserFld1,'') else ENDCUST.CustName end
else NA.Description  end) as [EndCustName]
,CASE SP.SperID 	
When 'HA' THEN 'House'
When 'SP1' THEN 'HH'
When 'SP2' THEN 'Transit'
When 'SP3' THEN 'HH'
When 'SP4' THEN 'HH'
When 'SP5' THEN 'Transit'
When 'SP6' THEN 'HH'
When 'SP7' THEN 'Transit'
When 'SP9' THEN 'House'
When 'SP8' THEN 'House'
When 'SP10' THEN 'HH'
ELSE ''
END	
As CustType
,'' as LineCmnt
,ISNULL(SO.TranCmnt,'') as HdrCmnt
FROM dbo.tarInvoice as INV WITH (nolock) 
left outer join dbo.tarInvoiceDetl as INVD WITH (nolock) on INVD.InvcKey = INV.InvcKey
left outer join dbo.tarInvoiceLineDist as INVS WITH (nolock) on INVS.InvoiceLineKey = INVD.InvoiceLineKey
left outer join dbo.vFormattedGLAcct FGL ON FGL.GLAcctKey = INVS.SalesAcctKey 
left outer join dbo.tarCustomer as CUST WITH (nolock) on CUST.CustKey = INV.CustKey
LEFT OUTER JOIN tarCustAddr as CustAddr ON CUST.PrimaryAddrKey = CustAddr.AddrKey
LEFT OUTER JOIN tarSalesperson as SP ON CustAddr.SperKey = SP.SperKey 
left outer join dbo.tsoSOLineDist as SLD WITH (nolock) on SLD.SOLineDistKey = INVS.SOLineDistKey 
left outer join dbo.tsoSOLine as SL  WITH (nolock) on SL.SOLineKey = SLD.SOLineKey 
left outer join dbo.tsoSalesOrder as SO WITH (nolock) on SO.SOKey = SL.SOKey
left outer join dbo.tarcustomer as ENDCUST  WITH (nolock) ON ENDCUST.CustID = SO.UserFld1 and ENDCUST.CompanyId = So.CompanyId
left outer join dbo.tarNationalAcctLevel as NAL  WITH (nolock) 
    on case when ENDCUST.NationalAcctLevelKey IS NULL THEN CUST.NationalAcctLevelKey ELSE ENDCUST.NationalAcctLevelKey END = NAL.NationalAcctLevelKey
left outer join dbo.tarNationalAcct as NA on NA.NationalAcctKey = NAL.NationalAcctKey
WHere INV.CompanyID  = 'USA'
AND FGL.FormattedGLAcctNo BETWEEN '40000' and '49999'
AND INV.ClosingPostDate &gt;=@startdate 
AND INVS.SOLineDistKey  is not null


