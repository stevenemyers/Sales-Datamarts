            <component
              refId="Package\Update Main Datamart\Freight"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Freight"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
YEAR(inv.PostDate) as Year
,Month(inv.PostDate) as Month
,cust.CustID, cust.CustName,
rtrim(case when ISNULL(NA.NationalAcctId, '') = '' then CASE WHEN ISNULL(SO.UserFld1,'') = '' THEN cust.CustID ELSE SO.UserFld1 END
else NA.NationalAcctId end) as 'EndCust',
rtrim(case when ISNULL(NA.NationalAcctId, '') = ''  then case when ISNULL(ENDCUST.CustName, '') = ''  then 
CASE WHEN ISNULL(SO.UserFld1,'') = '' THEN cust.CustName ELSE SO.UserFld1 END else ENDCUST.CustName end
else NA.Description  end) as 'EndCustName',
1 as QtyShipped,
(INVS.FreightAmt * INv.CurrExchRate) as USDAmt,
0 as USDEach,
INV.CurrID,
INv.CurrExchRate,
(INVS.FreightAmt ) as LocalAmt,
'FREIGHT' As ItemID, 
'FREIGHT' As Description, 
'MISC SUPPLY' AS ItemClassID, 
'MISC SUPPLY' AS ItemClassName, 
--SM 2016-12-08 Added Qty so report can be viewed by $$$ and Qty 
inv.PostDate as TranDate
,CAST(inv.TranID as VarChar(20)) as TranID
,INVS.InvoiceLineKey
, 	inv.TranType, Itm.ItemType
,ISNULL(ADDR.StateID,'') as State
,ISNULL(ADDR.CountryID,'') as Country
,ISNULL(ADDR.AddrName,'')  as [ShipName]
,ISNULL(BillAddr.StateID,'') as [State BillTo]
,ISNULL(BillAddr.CountryID,'') as [Country BillTo] 
, 45024  as GLAcctNo
, 'SALES - FACT FRT' as GLDescription
,'' as CustType
,'3' as Source
,ISNULL(INVD.ExtCmnt,'') as LineCmnt
,ISNULL(INV.TranCmnt,'') as HdrCmnt
from dbo.tarInvoice as INV WITH (nolock)
left outer join dbo.tarInvoiceDetl as INVD WITH (nolock) on INVD.InvcKey = INV.InvcKey
left outer join dbo.tarInvoiceLineDist as INVS WITH (nolock) on INVS.InvoiceLineKey = INVD.InvoiceLineKey	
left outer join dbo.vFormattedGLAcct FGL ON FGL.GLAcctKey = INVS.SalesAcctKey 
left outer join dbo.tarCustomer as CUST WITH (nolock) on CUST.CustKey = INV.CustKey
left outer join dbo.timItem as ITM WITH (nolock) on ITM.ItemKey = INVD.ItemKey
left outer join dbo.timItemClass as ITC WITH (nolock) on ITC.ItemClassKey = ITM.ItemClassKey
--SM 2016/12/08  added below to include end cust 
left outer join dbo.tsoSOLineDist as SLD WITH (nolock) on SLD.SOLineDistKey = INVS.SOLineDistKey 
left outer join dbo.tsoSOLine as SL  WITH (nolock) on SL.SOLineKey = SLD.SOLineKey
left outer join dbo.tsoSalesOrder as SO WITH (nolock) on SO.SOKey = SL.SOKey
left outer join dbo.tarcustomer as ENDCUST  WITH (nolock) ON ENDCUST.CustID = SO.UserFld1 and ENDCUST.CompanyId = So.CompanyId
Left OUter Join [dbo].[tciAddress] as ADDR 	WITH (nolock)  on INV.ShipToAddrKey = ADDR.AddrKey
Left OUter Join [dbo].[tciAddress] as BillAddr 	WITH (nolock)  on INV.BillToAddrKey = BillAddr.AddrKey

left outer join dbo.tarNationalAcctLevel as NAL  WITH (nolock) 
    on case when ENDCUST.NationalAcctLevelKey IS NULL THEN CUST.NationalAcctLevelKey ELSE ENDCUST.NationalAcctLevelKey END = NAL.NationalAcctLevelKey
left outer join dbo.tarNationalAcct as NA on NA.NationalAcctKey = NAL.NationalAcctKey

where inv.CompanyID = 'usa' and inv.PostDate  &gt;= ? and inv.PostDate &lt;= ?
--and  itm.ItemType in (5,8)
--AND inv.TranType BETWEEN 502 AND 520
AND FGL.FormattedGLAcctNo BETWEEN '40000' and '49999'
AND INVS.FreightAmt &lt;&gt; 0</property>
