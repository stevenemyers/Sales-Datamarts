                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
YEAR(GT.PostDate) as Year
,Month(GT.PostDate) as Month
,CustID=''	,CustName=''	,EndCust=''	,EndCustName=''	,QtyShipped=0
,SUM(GT.PostAmtHC * -1) AS USDAmt
,0 AS USDEach
,GT.CurrID
,GT.CurrExchRate
,SUM(GT.PostAmt * -1) as LocalAmt
,ItemID=''	
,Description = ''
,ItemClassID=''	
, ItemClassName = ''
,TranDate= GT.PostDate 
, (TT.TranTypeID) AS TranID
, GT.glTranKey
,  GT.TranType
,ItemType=''	,State=''	,Country=''
,[ShipName] ='', [State BillTo] = '', [Country BillTo] = ''

,(GA.GLAcctNo) AS GLAcctNo
, MAX(GA.Description) as GLDescription
,'' as CustType
,'2' as [Source]
,'' as LineCmnt
,GT.PostCmnt as HdrCmnt 
FROM         tglAccount AS GA WITH (nolock) LEFT OUTER JOIN
                      tglTransaction AS GT WITH (nolock)  ON GT.GLAcctKey = GA.GLAcctKey LEFT OUTER JOIN
                      tciTranTypeCompany AS TT WITH (nolock) ON TT.TranType = GT.TranType AND TT.CompanyID = 'USA'
WHERE     (GA.CompanyID = 'usa') AND (GA.GLAcctNo BETWEEN '40000' AND '49999') AND (GT.PostAmtHC &lt;&gt; 0) 
AND  (GT.TranType IN (303,401,402,708,710,811) OR GT.TranType IS NULL ) 
and GT.PostDate  &gt;= ?  and GT.PostDate &lt;= ?

Group By Month(GT.PostDate) ,YEAR(GT.PostDate), TT.TranTypeID ,  GT.TranType,  GT.glTranKey
, GA.GLAcctNo, GT.CurrID ,GT.CurrExchRate
, GT.PostDate,GT.PostCmnt</property>
