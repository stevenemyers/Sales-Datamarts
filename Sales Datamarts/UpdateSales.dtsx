INSERT INTO @saleshist 
SELECT
YEAR(GT.PostDate) as Year
,Month(GT.PostDate) as Month
,CustID=isnull(INVEC.CustID,'')	,CustName=ISNULL(INVEC.CustName,'') ,EndCust=ISNULL(INVEC.EndCust,''),
EndCustName=ISNULL(INVEC.EndCustName,'')	,QtyShipped=0
,SUM(GT.PostAmtHC *  -1) AS USDAmt
, 0 As USDEach
,GT.CurrID
,GT.CurrExchRate
,SUM(GT.PostAmt * -1) as LocalAmt
,ItemID=''	
,Description = ''
,ItemClassID=''	
,ItemClassName = ''
--,TranDate= DATEADD(DAY,1 - DAY(GT.PostDate) ,GT.PostDate) 
,TranDate= GT.PostDate 
--, GT.BatchKey AS TranID
, GT.TranNo AS TranID
,GT.glTranKey
,  GT.TranType
,ItemType=''	,State=''	,Country=''
,[ShipName] ='', [State BillTo] = '', [Country BillTo] = ''
,(GA.GLAcctNo) AS GLAcctNo
, MAX(GA.Description) as GLDescription
,ISNULL(INVEC.CustType,'') as CustType
,'4' as Source
,ISNULL(INVEC.LineCmnt,'') as LineCmnt
,ISNULL(INVEC.HdrCmnt,GT.PostCmnt) as HdrCmnt 
FROM tglTransaction AS GT With (nolock)
LEFT OUTER JOIN tglAccount AS GA With (nolock) ON GT.GLAcctKey = GA.GLAcctKey 
LEFT OUTER JOIN tciTranTypeCompany AS TT With (nolock) ON TT.TranType = GT.TranType AND TT.CompanyID = 'USA'
LEFT OUTER JOIN @invec as INVEC on INVEC.TranNo = GT.TranNo 

WHERE     (GA.CompanyID = 'usa') AND (GA.GLAcctNo BETWEEN '47016' AND '47016') AND (GT.PostAmtHC &lt;&gt; 0) 
--AND  not  (GT.TranType BETWEEN 511 AND 503)
AND GT.PostDate  &gt;= @startdate and GT.PostDate &lt;= @enddate and 
(GT.TranType = 511  OR
(GT.TranType = 502 AND  GT.TranNo IN ('0000100036', '0000109774', '0000096095', '0000118597')  ) ) 

--Group By Month(GT.PostDate) ,YEAR(GT.PostDate), GT.BatchKey ,  GT.TranType, GA.GLAcctNo ,GT.CurrID,GT.CurrExchRate
Group By Month(GT.PostDate) ,YEAR(GT.PostDate), GT.TranNo ,GT.glTranKey
,  GT.TranType, GA.GLAcctNo ,GT.CurrID,GT.CurrExchRate
--, DATEADD(DAY,1 - DAY(GT.PostDate) ,GT.PostDate)
,GT.PostDate 
,ISNULL(INVEC.CustID,''),ISNULL(INVEC.CustName,''),ISNULL(INVEC.EndCust,''),ISNULL(INVEC.EndCustName,''), INVEC.CustType
,ISNULL(INVEC.LineCmnt,'') ,ISNULL(INVEC.HdrCmnt,GT.PostCmnt)

